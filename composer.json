{
    "name": "teikk/sage-hook-annotations",
    "description": "Hook service provider for Roots Sage theme development",
    "keywords": [
        "teikk",
        "sage-hook-annotations"
    ],
    "homepage": "https://github.com/teikk/sage-hook-annotations",
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "Konrad Łyżwa",
            "email": "k.lyzwa7@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.1",
        "kerryrandolph/wp-hook-annotations": "^0.0.3",
        "roots/acorn": "^3.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "Teikk\\SageHookAnnotations\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "acorn": {
            "providers": [
                "Teikk\\SageHookAnnotations\\Providers\\HookServiceProvider"
            ]
        }
    }
}
